        <script><![CDATA[(function() {
	gs.addInfoMessage("record producer");
	//-------------------------
	// Adobe Sign objects
	//-------------------------
	
	// Create an object to assist with the signing process
	var adobe = new x_adosy_as.AdobeSign();
	adobe.setAgreementRecord(current);
	
	// Add Source documents to the agreement
	// (We can pass in either the sys_id of a template, or the sys_id of an attachment)
	//
	var docs = (producer.templates + '').split(',');
	docs.forEach(function(doc_id) {
		adobe.addDocument(doc_id);
	});
	
	// Set the name of the Agreement to be the name of the first document
	var doc_names = (producer.templates.getDisplayValue() + '').split(',');
	current.name = doc_names[0];
	
	// Add the signers
	// (We can pass in either the sys_id of a user, or an email address as a string)
	//
	var users = (producer.users + '').split(',');
	users.forEach(function(user_id) {
		adobe.addSigner(user_id);
	});
	
	// Set the URL to redirect the singer to after signing
	// Finally set the Agreement state to Ready to submit it to Adobe Sign
	//
//	current.post_sign_uri = '/x_adosy_as_agreement.do?sys_id=' + current.sys_id + '&sysparm_view=x_adosy_as_end_user';
	current.post_sign_uri = 'http://intranet.dsnyad.nycnet/Intranet/Home/';
	
	
	current.state = 'Ready';
	
})();]]></script>
